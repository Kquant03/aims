# AIMS Environment Configuration
# Generated by setup script on <timestamp>

# Required - Add your API key here
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Optional but recommended
OPENAI_API_KEY=your-openai-api-key-for-embeddings
SESSION_SECRET=<generate-with-setup-script>

# Database Configuration
POSTGRES_PASSWORD=aims_secure_password_<random>
POSTGRES_HOST=localhost
POSTGRES_PORT=5433
POSTGRES_DB=aims_memory
POSTGRES_USER=aims

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333

# S3 Backup (optional)
S3_BACKUP_ENABLED=false
S3_BUCKET=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_REGION=us-east-1

# System Configuration
LOG_LEVEL=INFO
BACKUP_INTERVAL_HOURS=6
MAX_LOCAL_BACKUPS=7

# GPU Configuration
CUDA_VISIBLE_DEVICES=0
GPU_MEMORY_FRACTION=0.9
